{
  "name": "@sap-cloud-sdk/currency-conversion-integration-objects",
  "version": "1.0.0",
  "description": "Currency conversion library for Integeration Objects",
  "dependencies": {
    "@sap/audit-logging": "latest",
    "@sap/cds": "^4",
    "@sap/cds-dk": "^3.2.0",
    "@sap/hana-client": "^2",
    "@sap/hdi-deploy": "^3",
    "@sap/xsenv": "latest",
    "@sap/xssec": "^2",
    "@std/esm": "^0.26.0",
    "cds": "^0.2.0",
    "eslint": "^7.16.0",
    "express": "^4",
    "lint": "^0.7.0",
    "passport": "latest"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^26.5.3",
    "sqlite3": "^5"
  },
  "scripts": {
    "compile": "cds build/all --clean && cds compile srv/ --to xsuaa  > cds-security.json",
    "prepare": "yarn compile",
    "test": "yarn jest --coverage",
    "test:local": "yarn jest",
    "coverage": "yarn jest --coverage",
    "start": "yarn cds run"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "db": {
        "kind": "hana"
      }
    }
  },
  "keywords": [
    "currency"
  ],
  "author": "SAP",
  "license": "ISC"
}
